{
  "name": "nodebackend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "nodemon --exec babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "npm-run-all clean build server:prod"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validation": "^1.0.2",
    "http-status": "^1.3.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.6",
    "mongoose-sequence": "^5.0.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql2": "^1.6.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "sequelize": "^5.8.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-latest-node": "^3.2.1",
    "nodemon": "^1.19.0"
  }
}
